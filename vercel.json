{
	"version": 2,
	"builds": [
		{
			"src": "index.js",
			"use": "@vercel/node"
		},
		{
			"src": "public/**",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "index.js"
		},
		{
			"src": "/adopcion",
			"dest": "public/adopcion.html"
		},
		{
			"src": "/adoptar",
			"dest": "public/adoptar.html"
		},
		{
			"src": "/comentarios",
			"dest": "public/comentarios.html"
		},
		{
			"src": "/suscripcion",
			"dest": "public/suscripcion.html"
		},
		{
			"src": "/login",
			"dest": "public/login.html"
		},
		{
			"src": "/(.+)",
			"dest": "public/$1"
		}
	]
}
